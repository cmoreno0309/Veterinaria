<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.veterinaria.proyecto.veterinaria">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".PrincipalActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:label="@string/lbl_historia"
            android:name=".HistoriaActivity" />
        <activity
            android:label="@string/lbl_mascota"
            android:name=".MascotasActivity" />
        <activity
            android:label="@string/Noticias"
            android:name=".NoticiaActivity" />
        <activity
            android:label="@string/title_activity_contactenos"
            android:name=".Contactenos" />
        <activity
            android:label="@string/title_activity_descripcion_mascota"
            android:name=".DescripcionMascota"/>
        <activity
            android:label="@string/title_activity_registro_mascota"
            android:name=".RegistroMascota" />
        <activity
            android:label="@string/title_activity_registro_cita"
            android:name=".RegistroCita"/>
        <activity
            android:label="@string/lbl_logueo"
            android:name=".LogueoActivity"/>
        <activity
            android:label="@string/lbl_duenio"
            android:name=".DuenoActivity"/>
        <activity
            android:label="@string/lbl_servicios"
            android:name=".ServicioActivity"/>
        <activity
            android:label="@string/lbl_cuenta"
            android:name=".RegistroCuenta"/>
        <activity
            android:label="@string/lbl_recordatorio"
            android:name=".RecordatorioActivity"/>

    </application>

</manifest>
